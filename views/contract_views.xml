<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_contratos_electricos_tree" model="ir.ui.view">
            <field name="name">contratos.electricos.tree</field>
            <field name="model">contratos.electricos</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Contratos de Suministro">
                    <field name="nombre_contrato"/>
                </tree>
            </field>
        </record>
        
        <record model="ir.actions.act_window" id="contratos.action_window">
            <field name="name">contratos_electricos_window</field>
            <field name="res_model">contratos.electricos</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Contratos" id="contratos.menu_root"/>
        <menuitem name="Contratos" id="contratos.contratos" parent="contratos.menu_root"/>
        <menuitem name="Contratos tree" id="contratos.contratos_tree" parent="contratos.contratos" 
                action="contratos.action_window"/>
    </data>
</odoo>